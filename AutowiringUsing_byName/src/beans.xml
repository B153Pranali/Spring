<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
        
        <bean id="s" class="com.model.Student" autowire="byType">
        <property name="rollno" value="1"></property>
        <property name="name" value="pvd"></property>
       
        </bean>
        <bean id="addr" class="com.model.Address">
        <property name="cityName" value="Pune"></property>
        <property name="areaName" value="karve"></property>
        </bean>
        </beans>